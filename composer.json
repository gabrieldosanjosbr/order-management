{
    "name": "symfony/skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A minimal Symfony project recommended to create bare bones applications",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^1.11",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "2.7.x-dev",
        "nelmio/cors-bundle": "^1.5",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/console": "^3.4.30|^4.3.3",
        "symfony/dotenv": "^3.4.30|^4.3.3",
        "symfony/flex": "1.x-dev",
        "symfony/framework-bundle": "^3.4.30|^4.3.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/security-bundle": "^4.2",
        "symfony/serializer": "5.4.x-dev",
        "symfony/translation": "^4.3@beta",
        "symfony/yaml": "^3.4.30|^4.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "symfony/browser-kit": "^3.4.30|^4.3.3",
        "symfony/css-selector": "^3.4.30|^4.3.3",
        "symfony/debug-bundle": "4.4.x-dev",
        "symfony/maker-bundle": "v1.39.x-dev",
        "symfony/phpunit-bridge": "7.1.x-dev",
        "symfony/stopwatch": "^3.4.30|^4.3.3",
        "symfony/web-profiler-bundle": "^3.4.30|^4.3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*",
        "symfony/runtime": "*"
    },
    "scripts": {
        "auto-scripts": {},
        "post-install-cmd": [],
        "post-update-cmd": []
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
